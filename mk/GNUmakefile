#	This Makefile creates the file mk/include/CONFIG.mk which
#	establishes defaults overriding the parameters defined in
#	include/mk/defaults.mk. Note that this Makefile does not
#	include common.mk or targets.mk.

DEPTH = ..

include $(DEPTH)/mk/include/defaults.mk

CONFIG_FILE = $(DEPTH)/mk/include/CONFIG.mk

.PHONY:	all clean clobber lint

all:

clean:

clobber:

clobberall:
	-rm -f $(CONFIG_FILE)

install:

lint:

.PHONY:	config par_check

par_check:

config: par_check clobber $(CONFIG_FILE)

$(CONFIG_FILE):
	@echo "# "CONFIG.mk" - generated by 'make config'" > $(CONFIG_FILE)
	@echo "" >> $(CONFIG_FILE)
	@echo "DEBUG ?= "$(DEBUG) >> $(CONFIG_FILE)
	@echo "COMPLETE ?= "$(COMPLETE) >> $(CONFIG_FILE)

include $(DEPTH)/mk/depend.mk
