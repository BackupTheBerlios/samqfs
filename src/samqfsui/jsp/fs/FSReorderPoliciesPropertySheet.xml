<!-- $Id: FSReorderPoliciesPropertySheet.xml,v 1.11 2008/05/16 19:39:19 am143972 Exp $ -->

<propertysheet>
    <section name="reorder" defaultValue="">        

    <property span="true">
	&lt;table border="0">
	&lt;tr>
	&lt;td valign="center" rowspan="2" colspan="1">
        <cc name="saveType"
            tagclass="com.sun.web.ui.taglib.html.CCRadioButtonTag" >
            <option label="" value="reorder" />
            <option label="" value="savefile" />
            <attribute name="styleLevel" value="3" />
            <attribute name="elementId" value="saveTypeRadio"/>
            <attribute name="onClick" value="
                if (this.id == 'saveTypeRadio') {
                    ccSetTextFieldDisabled(
                        'FSReorderPolicies.fileLocation',
                        'FSReorderPoliciesForm',
                        true);
                } else if (this.id=='saveTypeRadio2') {
                    ccSetTextFieldDisabled(
                        'FSReorderPolicies.fileLocation',
                        'FSReorderPoliciesForm',
                        false);
                    fileLocationTextFieldBox.focus(); 
                }"
            />
        </cc>
	&lt;/td>
	&lt;td valign="center" align="left" rowspan="1" colspan="1">
	<cc name="reorderText"
            tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag" >
            <attribute name="defaultValue"
                value="FSReorderPolicies.reorderCriteria"/>
	</cc>
	&lt;/td>
	&lt;/tr>

	&lt;tr>
        &lt;td>
	<cc name="saveToFile" 
            tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag" >   
            <attribute name="defaultValue"
                value="FSReorderPolicies.saveInDirectory"/>   
        </cc>
	<cc name="fileLocation"
            tagclass="com.sun.web.ui.taglib.html.CCTextFieldTag" >
            <attribute name="onKeyPress"
                value="if (event.keyCode == 13) return false" />
            <attribute name="elementId" value="fileLocationTextFieldBox"/>
            <attribute name="disabled" value="true" />
            <attribute name="dynamic" value="true" />
            <attribute name="maxLength" value="127" />
	</cc>
	 &lt;/td>
	&lt;/tr>
	&lt;/table>
	
    </property>
	
    </section>
</propertysheet>
