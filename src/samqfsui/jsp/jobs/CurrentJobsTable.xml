    <option label="Jobs.filterOptions1" value="1" />
    <option label="Jobs.filterOptions5" value="5" />
    <option label="Jobs.filterOptions2" value="2" />
    <option label="Jobs.filterOptions3" value="3" />
    <option label="Jobs.filterOptions6" value="6" />
    <option label="Jobs.filterOptions7" value="7" />
    <option label="Jobs.filterOptions.dump" value="dump" />
    <option label="Jobs.filterOptions.dumpEnable" value="dumpEnable" />
    <option label="Jobs.filterOptions.restoreSearch" value="restoreSearch" />
    <option label="Jobs.filterOptions.restore" value="restore" />
    </cc>
</filters>

<column name="Col0" sortname="Text0" extrahtml="nowrap">
  <cc name="JobIdHref" tagclass="com.sun.web.ui.taglib.html.CCHrefTag">
    <attribute name="submitFormData" value="true"/>
