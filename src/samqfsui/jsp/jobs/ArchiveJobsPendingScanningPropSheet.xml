<propertysheet>
	<section name="scanningJob" defaultValue="">

	<property span="true" required="false">
            <label name="jobId" defaultValue="JobsDetails.jobId"/>
	    <cc name="jobIdText" 
	    tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag">
            <attribute name="defaultValue" value=""/>
	    </cc>
	</property>

	<property span="true" required="false">
            <label name="jobType" defaultValue="JobsDetails.jobType"/>
	    <cc name="jobTypeText" 
	    tagclass="com.sun.web.ui.taglib.html.CCStaticTextFieldTag">
            <attribute name="defaultValue" value=""/>
	    </cc>
	</property>

	<property span="true">
	<cc name="pendingScanningInfo" 
	tagclass="com.sun.web.ui.taglib.table.CCActionTableTag" >
     	<attribute name="title" value="JobsDetails.scanningTitle" />
	<attribute name="showAdvancedSortIcon" value="false" />
	<attribute name="showPaginationControls" value="false" />
	<attribute name="showLowerActions" value="false" />
	<attribute name="showPaginationIcon" value="false" />
	<attribute name="showSelectionIcons" value="false" />
	</cc>
	</property>
	</section>
</propertysheet>
