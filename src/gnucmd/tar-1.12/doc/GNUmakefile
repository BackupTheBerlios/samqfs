	rm -f tar.aux tar.cp tar.cps tar.dvi tar.fn tar.fns tar.ky tar.log \
	  tar.pg tar.toc tar.tp tar.tps tar.vr tar.vrs tar.op tar.tr \
	  tar.cv

clean-info:

distclean-info:

maintainer-clean-info:
	for i in $(INFO_DEPS); do rm -f `eval echo $$i*`; done
tags: TAGS
TAGS:

